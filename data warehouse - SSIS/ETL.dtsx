    public override void Entrada0_ProcessInputRow(Entrada0Buffer Row)
    {
        if (!Row.spotify_track_uri_IsNull)
        {
            try
            {
                using (var client = new HttpClient())
                {
                    // URL base de la API
                    string url = "http://localhost:8000/track-id/" + Row.spotify_track_uri;
                    HttpResponseMessage response = client.GetAsync(url).Result;

                    if (response.IsSuccessStatusCode)
                    {
                        string json = response.Content.ReadAsStringAsync().Result;
                        JObject data = JObject.Parse(json);

                        // Asignar valores desde el JSON
                        Row.api_popularity = (int?)data["popularity"] ?? 0;
                        Row.api_genre = (string)data["genre"] ?? "Desconocido";
                        Row.api_release_year = (int?)data["release_year"] ?? 0;
