                    else
                    {
                        Row.api_popularity = 0;
                        Row.api_genre = "Sin datos";
                    }
                }
            }
            catch (Exception ex)
            {
                Row.api_popularity = 0;
                Row.api_genre = "Error: " + ex.Message;
            }
        }
    }

